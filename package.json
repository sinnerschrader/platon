{
  "name": "platon",
  "version": "1.0.0",
  "description": "Create universals design systems from archetypes",
  "private": true,
  "scripts": {
    "start": "lerna run --stream --no-sort start",
    "lerna": "lerna",
    "start:sketch": "lerna run --stream --no-sort start --scope=platon-sketch",
    "start:browser": "lerna run --stream --no-sort start --scope=platon-browser",
    "postinstall": "npm run lerna bootstrap",
    "reinstall": "npm run lerna clean && npm install"
  },
  "author": "SinnerSchrader AG <jobs@sinnerschrader.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "lerna": "^2.0.0-rc.5"
  }
}
